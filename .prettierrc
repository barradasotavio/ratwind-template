{
    "trailingComma": "all",
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "printWidth": 80,
    "bracketSpacing": true,
    "arrowParens": "always",
    "bracketSameLine": true,
    "quoteProps": "consistent",
    "proseWrap": "always",
    "plugins": ["prettier-plugin-tailwindcss"],
    "singleAttributePerLine": true,
    "experimentalTernaries": true
}